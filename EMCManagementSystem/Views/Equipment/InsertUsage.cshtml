
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>InsertUsage</title>
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/font-awesome.css" rel="stylesheet" />

    <link href="~/Content/css/jquery-ui.custom.min.css" rel="stylesheet" />
    <link href="~/Content/css/chosen.css" rel="stylesheet" />
    <link href="~/Content/css/datepicker.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap-timepicker.css" rel="stylesheet" />
    <link href="~/Content/css/daterangepicker.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap-datetimepicker.css" rel="stylesheet" />
    <link href="~/Content/css/colorpicker.css" rel="stylesheet" />
    <link href="~/Content/css/ace-fonts.css" rel="stylesheet" />
    <link href="~/Content/css/ace.min.css" rel="stylesheet" id="main-ace-style" />
</head>
<body class="no-skin">
    <!-- #section:basics/navbar.layout -->
    <!-- /section:basics/navbar.layout -->
    <div class="main-container" id="main-container">

        <!-- /section:basics/sidebar -->
        <div class="main-content">

            <!-- /section:basics/content.breadcrumbs -->
            <div class="page-content">

                <div class="page-content-area">

                    <div class="row">
                        <div class="col-xs-12">
                            <!-- PAGE CONTENT BEGINS -->
                            <form class="form-horizontal" role="form">
                                <!-- #section:elements.form -->
                                <div class="form-group">
                                    <label class="col-sm-3 control-label no-padding-right" for="form-field-1-1">使用人姓名： </label>

                                    <div class="col-sm-9">
                                        <input type="text" id="form-field-1-1" class=" col-xs-10 col-sm-5" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> 仪器设备名称：</label>

                                    <div class="col-sm-9">
                                        <input type="text" id="form-field-1" class="col-xs-10 col-sm-5" />
                                    </div>
                                </div>



                                <!-- /section:elements.form -->
                                <div class="space-4"></div>

                                <div class="form-group">
                                    <label class="col-sm-3 control-label no-padding-right" for="form-field-2">使用日期： </label>

                                    <div class="col-sm-9">
                                        <input readonly="" type="text" id="form-field-2" class="col-xs-10 col-sm-5" value="2017/10/26" />
                                    </div>
                                </div>

                                <div class="space-4"></div>

                                <div class="form-group">
                                    <label class="col-sm-3 control-label no-padding-right" for="form-input-readonly">使用前是否正常： </label>

                                    <div class="col-sm-9">


                                        <div class="radio">
                                            <label>
                                                <input name="form-field-radio" type="radio" class="ace" />
                                                <span class="lbl"> 是</span>
                                            </label>
                                        </div>

                                        <div class="radio">
                                            <label>
                                                <input name="form-field-radio" type="radio" class="ace" />
                                                <span class="lbl"> 否</span>
                                            </label>
                                        </div>

                                    </div>
                                </div>
                                <div class="space-4"></div>

                                <div class="form-group">
                                    <label class="col-sm-3 control-label no-padding-right" for="form-input-readonly">使用后是否正常： </label>

                                    <div class="col-sm-9">


                                        <div class="radio">
                                            <label>
                                                <input name="form-field-radio" type="radio" class="ace" />
                                                <span class="lbl"> 是</span>
                                            </label>
                                        </div>

                                        <div class="radio">
                                            <label>
                                                <input name="form-field-radio" type="radio" class="ace" />
                                                <span class="lbl"> 否</span>
                                            </label>
                                        </div>

                                    </div>
                                </div>
                                @*<div class="space-4"></div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label no-padding-right" for="id-date-picker-1">加班起始时间： </label>

                                        <div class="col-sm-9">
                                            <input id="date-timepicker1" type="text" class="col-xs-10 col-sm-5" />

                                        </div>


                                    </div>
                                    <div class="space-4"></div>

                                    <div class="form-group">
                                        <label class="col-sm-3 control-label no-padding-right" for="id-date-picker-1">加班结束时间： </label>

                                        <div class="col-sm-9">
                                            <input id="date-timepicker2" type="text" class="col-xs-10 col-sm-5" />

                                        </div>


                                    </div>*@
                                <div class="space-4"></div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label no-padding-right" for="limited">备注：</label>
                                    <div class="col-sm-9">
                                        <div class="pos-rel">
                                            <textarea class="form-control limited autosize-transition" id="limited" maxlength="50"></textarea>
                                        </div>
                                    </div>
                                </div>
                                <div class="clearfix form-actions" style="background:#fff">
                                    <div class="col-md-offset-3 col-md-9">
                                        <button class="btn btn-info" type="button" style="float:right">
                                            <i class="ace-icon fa fa-check bigger-110"></i>
                                            立即提交
                                        </button>

                                        &nbsp; &nbsp; &nbsp;
                                        <button class="btn" type="reset" style="float:right">
                                            <i class="ace-icon fa fa-undo bigger-110"></i>
                                            重置
                                        </button>
                                    </div>
                                </div>

                            </form>

                        </div><!-- /.col -->
                    </div><!-- /.row -->
                </div><!-- /.page-content-area -->
            </div><!-- /.page-content -->
        </div><!-- /.main-content -->
    </div><!-- /.main-container -->
    <!-- basic scripts -->
    <!--[if !IE]> -->

    <script src="~/Content/js/jquery-1.8.3.min.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>


    <!-- page specific plugin scripts -->
    <!--[if lte IE 8]>
      <script src="assets/js/excanvas.min.js"></script>
    <![endif]-->

    <script src="~/Content/js/date-time/bootstrap-datepicker.min.js"></script>
    <script src="~/Content/js/date-time/bootstrap-timepicker.min.js"></script>
    <script src="~/Content/js/date-time/moment.min.js"></script>
    <script src="~/Content/js/date-time/daterangepicker.min.js"></script>
    <script src="~/Content/js/date-time/bootstrap-datetimepicker.min.js"></script>
    <script type="text/javascript">
			jQuery(function($) {

			    $('#date-timepicker1').datetimepicker().next();
			    $('#date-timepicker2').datetimepicker().next();
			});
    </script>
</body>
</html>
